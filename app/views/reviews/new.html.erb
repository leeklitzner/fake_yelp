<h1>New Review</h1>
<%= form_for @review do |f| %>
  <%= f.label :name, "Restaurant Name" %>
  <%= f.text_field :name %>

  <%= f.label :chef %>
  <%= f.text_field :chef %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>

  <%= f.label :type_of_food %>
  <%= f.text_area :tye_of_food %>

  <%= f.submit %>

<% end %>
